{
    "plugins": [
        "plugins/markdown"
    ],
    "markdown": {
        "tags": [
        ]
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": false,
        "dictionaries": ["jsdoc"]
    },
    "templates": {
        "cleverLinks": true
    },
    "opts": {
        "destination": "./doc/api",
        "encoding": "utf8",
        "recurse": true,
        "verbose": true,
        "template": "./node_modules/docdash",
        "package": "./package.json",
        "readme": "./README.md"
    },
    "docdash": {
        "static": true,
        "sort": true,
        "search": true,
        "collapse": true,
        "typedefs": true,
        "private": false
    }
}
