{
  "name": "twenty-one-pips",
  "version": "0.0.0",
  "description": "A Dice Game Engine for Creating Dice Games",
  "main": "lib/twenty-one-pips.js",
  "directories": {
    "doc": "doc",
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "test": "node_modules/.bin/mocha --require babel-register --recursive --es_staging --harmony",
    "doc": "node_modules/.bin/jsdoc ./src --destination doc/api --recurse --template node_modules/docdash --package ./package.json",
    "examples": "node_modules/.bin/http-server examples/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/htdebeer/twenty-one-pips.git"
  },
  "keywords": [
    "dice",
    "dice",
    "game",
    "die"
  ],
  "author": "Huub de Beer <Huub@heerdebeer.org>",
  "license": "LGPL-3.0",
  "bugs": {
    "url": "https://github.com/htdebeer/twentyonepips/issues"
  },
  "homepage": "https://github.com/htdebeer/twentyonepips#readme",
  "devDependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "docdash": "^0.4.0",
    "eslint": "^4.19.1",
    "http-server": "^0.11.1",
    "jsdom": "11.7.0",
    "jsdom-global": "3.0.2",
    "mocha": "^5.0.5"
  },
  "dependencies": {
    "npm": "^5.8.0"
  }
}
